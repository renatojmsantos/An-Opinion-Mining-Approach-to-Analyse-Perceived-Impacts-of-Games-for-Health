	file = {Figueiredo et al. - 2011 - The tube over time characterizing popularity grow.pdf:/Users/renatosantos/Zotero/storage/7G36VIZL/Figueiredo et al. - 2011 - The tube over time characterizing popularity grow.pdf:application/pdf},
}

@book{cha_i_2007,
