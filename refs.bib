	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/jnr.24404},
	keywords = {mood disorders, natural language processing, social networks, suicidal behavior},
	pages = {616--625},
	file = {Full Text PDF:/Users/renatosantos/Zotero/storage/AW5T2PD5/Lopez-Castroman et al. - 2020 - Mining social networks to improve suicide preventi.pdf:application/pdf;Snapshot:/Users/renatosantos/Zotero/storage/BUZJ23Q2/jnr.html:text/html},
