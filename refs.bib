	author = {Caraban, Ana and Karapanos, Evangelos and Gonçalves, Daniel and Campos, Pedro},
	month = may,
	year = {2019},
	pages = {1--15},
	file = {Caraban et al. - 2019 - 23 Ways to Nudge A Review of Technology-Mediated .pdf:/Users/renatosantos/Zotero/storage/PRRYERTQ/Caraban et al. - 2019 - 23 Ways to Nudge A Review of Technology-Mediated .pdf:application/pdf},
}

