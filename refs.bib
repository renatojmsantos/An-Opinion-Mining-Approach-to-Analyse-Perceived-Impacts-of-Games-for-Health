	pages = {68--91},
	file = {ScienceDirect Full Text PDF:/Users/renatosantos/Zotero/storage/A8BT2UPU/Vassiliadis et al. - 2019 - Beyond roll-up’s and drill-down’s An intentional .pdf:application/pdf;ScienceDirect Snapshot:/Users/renatosantos/Zotero/storage/XXHNQDK4/S0306437918303016.html:text/html},
}

@article{nadal_integration-oriented_2019,
	series = {Special issue on {DOLAP} 2017: {Design}, {Optimization}, {Languages} and {Analytical} {Processing} of {Big} {Data}},
